#include <stdio.h>
#include "ft_printf.h"

int		main(int argc, char **argv)
{
	int a;

	a = 0;
	ft_atoi(argv[0]);
	//argv[1] = NULL;
	if (argc == 2)
	{

//	 a = dprintf(1, "%- *.95d \n", 22, 44);
	 a = printf("pf %2$.19d\n", 12, 4, -88);
	 printf("pf = %d\n", a);
  	 a = ft_printf("ft %2$.19d\n", 12, 4, -88);
	 printf("ft = %d\n", a);
	}
	return (0);
}
